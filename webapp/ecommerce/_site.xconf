<?xml version="1.0" encoding="UTF-8"?>

<page>
  <property name="fallbackdirectory">/WEB-INF/base/emshare</property>
  <property name="title">E-commerce</property>
  <property name="parentapplicationid">media</property>
  <property name="catalogid">media/catalogs/store</property>
  <property name="applicationid">ecommerce</property>
  <path-action name="Date.now" />
  <property name="continueShoppingUrl">/${applicationid}/browse.html</property>
  <style id="ecommerce-app-css" href="${themeprefix}/styles/ecommerce.css"/>
    <property name="productshowonly">+approved:true</property>
  	  	<property name="oetextfilter">xmltranslation</property>
  
  <permission name="viewdevelopertools">
  
  
    <or>
      <group id="administrators"/>
    </or>
  </permission>
  
  
  <permission name="maximize">
    <userprofile value="true" name="maximize"/>
  </permission>
  <permission name="publish">
    <userprofile value="true" name="publish"/>
  </permission>
  <permission name="backupmaterial">
    <boolean value="true"/>
  </permission>
  <permission name="purchaseinformation">
    <boolean value="true"/>
  </permission>
  <permission name="contactinformation">
    <boolean value="true"/>
  </permission>
  <permission name="description">
    <boolean value="true"/>
  </permission>
  <permission name="view">
    <user/>
  </permission>
  <permission name="deleteasset">
    <and>
      <userprofile value="true" name="deleteasset"/>
      <pagevalue equals="true" name="caneditasset"/>
    </and>
  </permission>
  
  <permission name="viewdistributorsection">
      <userprofile value="true" name="distributorsection"/>  
  </permission>
  
  
  <permission name="viewrogerstools">
    <or>
      <group id="administrators"/>
      <group id="rogersadministrators"/>
    </or>
  </permission>
  
   <permission name="vieweol">
    <or>
      <group id="administrators"/>
      <group id="distributors"/>
    </or>
  </permission>
  
   <permission name="manageorders">
    <or>
      <group id="administrators"/>
      
    </or>
  </permission>
 
  <permission name="edituserprofile">
   	<user/>
  </permission>
  
  <permission name="browseproducts">
      <userprofile value="true" name="browseproducts"/>
  </permission>
  
  <permission name="viewcart">
      <userprofile value="true" name="viewcart"/>
  </permission>
   
  <permission name="checkout">
    <userprofile value="true" name="purchase"/>
  </permission>
   
  <permission name="rogersprice">
      <userprofile value="true" name="rogersprice"/>
  </permission>
  
  <permission name="createwishlist">
      <userprofile value="true" name="wishlist"/>
  </permission>
  
  <permission name="viewtesting">
      <userprofile value="true" name="viewtesting"/>
  </permission>
  
   <permission name="dealertools">
      <userprofile value="true" name="dealertools"/>
  </permission>

  <permission name="viewaddresslist">
      <userprofile value="true" name="dealertools"/>
  </permission>
  
  <permission name="viewwishlists">
      <userprofile value="true" name="dealertools"/>
  </permission>

  <permission name="vieworderhistory">
      <userprofile value="true" name="dealertools"/>
  </permission>
  
	<path-action name="storeArchive.loadStore"/>
  	<path-action bean="DateStorageUtil" name="PageValue.loadPageVariable"  class="org.openedit.util.DateStorageUtil" />
    <path-action bean="Date" name="PageValue.loadPageVariable" allowduplicates="true" class="java.util.Date" />
	<script id="offers" src="/${applicationid}/events/scripts/home-page-offers.js"/>
	<script id="jquery-cycle" src="/${applicationid}/events/scripts/jquery.cycle.all.latest.js"/>
<property name="translationsid">media/catalogs/store</property>
	<path-action name="TranslationModule.loadTranslations"/>
	<property name="languages">
		<value locale="es">Espaï¿½ol</value>
		<value locale="FR">Francais</value>
		<value locale="DE">Deutsch</value>
		<value locale="nl">Dutch</value>
		
		<value>English</value>
	</property>
	<property name="auto_translate_FR">true</property>
		<property name="auto_translate_DE">true</property>
	
		<property name="auto_translate_es">true</property>
	
		<property name="auto_translate_FR">true</property>
			<property name="auto_translate_nl">true</property>
	
	
	
	
</page>
