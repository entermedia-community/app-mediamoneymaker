package org.openedit.store;

import org.openedit.money.Money;

/**
 * @author dbrown
 *
 */
public class HandlingCharge
{
	protected String fieldLevel;
	protected Money fieldCost = Money.ZERO;
	protected boolean fieldAdditionalCosts;

	public HandlingCharge()
	{
	}

	public Money getCost()
	{
		return fieldCost;
	}

	public void setCost(Money inCost)
	{
		fieldCost = inCost;
	}

	public String getLevel()
	{
		return fieldLevel;
	}

	public void setLevel(String inLevel)
	{
		fieldLevel = inLevel;
	}

	public boolean isAdditionalCosts()
	{
		return fieldAdditionalCosts;
	}

	public void setAdditionalCosts(boolean inAdditionalCosts)
	{
		fieldAdditionalCosts = inAdditionalCosts;
	}
}
